{
    "name": "simple-proxy",
    "version": "0.1.0",
    "description": "Simple HTTP proxy for node.js",
    "keywords": [
        "http",
        "proxy",
        "simple"
    ],
    "homepage": "https://github.com/ripe-tech/simple-proxy#readme",
    "bugs": {
        "url": "https://github.com/ripe-tech/simple-proxy/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ripe-tech/simple-proxy.git"
    },
    "license": "Apache-2.0",
    "author": "Platforme",
    "main": "app.js",
    "files": [
        "app.js",
        "lib/**/*.js",
        "test/**/*.js"
    ],
    "scripts": {
        "dev": "nodemon app.js",
        "lint": "eslint \"./**/*.js\"",
        "lint-fix": "eslint \"./**/*.js\" --fix",
        "prettier": "prettier \"./**/*.{js,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "start": "node app.js",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u"
    },
    "dependencies": {
        "consola": "^2.11.3",
        "express": "^4.17.1",
        "hive-js-util": "^0.2.2",
        "request": "^2.88.2",
        "yonius": "^0.2.21"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-hive": "^0.2.2",
        "mocha": "^7.1.1",
        "nodemon": "^2.0.3",
        "npm-check-updates": "^4.1.2",
        "prettier": "^2.0.4",
        "prettier-config-hive": "^0.1.7",
        "sort-package-json": "^1.41.0"
    }
}
